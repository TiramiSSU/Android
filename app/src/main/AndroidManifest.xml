<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ssuapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".TimetableActivity"></activity>
        <activity android:name=".EditActivity"></activity>
        <activity android:name=".authentication.AuthActivity"
            android:noHistory="true"></activity>
        <activity android:name=".cloudstorage.FileListActivity"></activity>
        <activity android:name=".cloudstorage.MetaInfoActivity"></activity>
        <activity android:name=".cloudstorage.UploadActivity"></activity>
        <activity android:name=".cloudstorage.DownloadActivity"></activity>
        <activity android:name=".cloudstorage.CloudStorageActivity"></activity>
        <activity
            android:name=".authentication.SignedInActivity"
            android:noHistory="true"></activity>
        <activity android:name=".ProjectListAddActivity"></activity>
        <activity android:name=".TimetableMainActivity"></activity>
        <activity android:name=".TimetableResultActivity"></activity>
        <activity android:name=".ProjectListActivity"></activity>
        <activity android:name=".ProjectManagementActivity"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>